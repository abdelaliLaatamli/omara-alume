<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="700.0" prefWidth="1200.0" style="-fx-background-color: #a8dadc;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.Controllers.ListCommandsController">
    <children>

        <HBox layoutX="14.0" layoutY="14.0" prefHeight="40.0" prefWidth="319.0">
            <children>
                <Button mnemonicParsing="false" onMouseClicked="#goBack" prefHeight="40.0" text="&lt;&lt;">
                    <font>
                        <Font name="Segoe UI Bold" size="15.0" />
                    </font>
                </Button>
                <Button mnemonicParsing="false" onMouseClicked="#goToCommandGenerator" prefHeight="40.0" text="Ajouter Commande">
                    <font>
                        <Font name="Segoe UI Bold" size="18.0" />
                    </font>
                </Button>

            </children>
        </HBox>
      <TableView fx:id="listCommandTable" layoutX="186.0" layoutY="63.0" prefHeight="630.0" prefWidth="1000.0">
        <columns>
            <TableColumn fx:id="referenceCommand" prefWidth="249.0" text="Reference Command" />
            <TableColumn fx:id="dateCommand" minWidth="4.0" prefWidth="99.0" text="Date Command" />
            <TableColumn fx:id="clientCommand" prefWidth="113.0" text="Client" />
            <TableColumn fx:id="paymentStatusCommand" minWidth="4.0" prefWidth="126.0" text="Payment Status" />
            <TableColumn fx:id="paymentsMadesOfCommand" prefWidth="98.0" text="payments Faits" />
            <TableColumn fx:id="articleCommands" prefWidth="116.0" text="Article Commands" />
            <TableColumn fx:id="editCommand" prefWidth="96.0" text="Edit Command" />
            <TableColumn fx:id="peuseCommand" prefWidth="102.0" text="Pause Command" />
        </columns>
      </TableView>


    </children>


</AnchorPane>
